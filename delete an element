#include<stdio.h>
int main()
{
  int a[50],size,pos;
  printf("enter the number of element\n");
  scanf("%d",&size);
  printf("enter the element\n");
  for(int i=0;i<size;i++)
    {
      scanf("%d",&a[i]);
    }
  printf("eneter the element position u want to delete\n");
  scanf("%d",&pos);
  for(int i=pos-1;i<size-1;i++)
    {
      a[i]=a[i+1];
      
    }
  size--;
  printf("the elements are\n");
  for(int i=0;i<size;i++)
    {
      printf("%d\n",a[i]);
    }
}
