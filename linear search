#include<stdio.h>
int main()
{
  int a[50],size,ele,i;
  printf("enetr the number of element\n");
  scanf("%d",&size);
  printf("enter the elements\n");
  for(int i=0;i<size;i++)
    {
      scanf("%d",&a[i]);
    }
  printf("enter the element to be searched\n");
  scanf("%d",&ele);
  for( i=0;i<size;i++)
    {
      if(a[i]==ele)
      {
        printf("element found at position:\n%d",i+1);
        break;
      }
    }
  if(i==size)
  {
    printf("element not found");
  }
}
